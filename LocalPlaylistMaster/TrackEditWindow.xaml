<Window x:Class="LocalPlaylistMaster.TrackEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocalPlaylistMaster"
        mc:Ignorable="d"
        Title="TrackEditWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="50" VerticalAlignment="Bottom">
            <Grid>
                <local:TrackPlayer x:Name="trackPlayer"/>
                <Slider Margin="50"/>
                <TextBlock x:Name="startText" Text="| Start" FontWeight="ExtraBold" Width="100" Height="20" Margin="52,25,603,-25" />
            </Grid>
        </StackPanel>
    </Grid>
</Window>
